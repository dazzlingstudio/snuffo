@model Uvendia.Domain.Mvc.ViewModels.CloudinaryFileModel
@{

}
@for (int i = 0; i < Model.AllCloudinaryFiles.Count; i++)
{
    <div class="image_block">
        <div class="image_block_inner">
            <div class="checkbox_box">
                <input name="@Model.AllCloudinaryFiles[i].Name" type="checkbox" class="image_selector" data-image-id="@Model.AllCloudinaryFiles[i].Name" data-image-url="@Model.AllCloudinaryFiles[i].Url" />
            </div>
            @if (Model.FileType != Uvendia.Domain.Enums.CloudinaryFileType.Image)
            {
                <div class="containerHtmlTag">
                    @Html.Raw(Model.AllCloudinaryFiles[i].HtmlTag)
                </div>
            }
            else
            {
                <img src="@Model.AllCloudinaryFiles[i].Url" alt="@Model.AllCloudinaryFiles[i].Name">
            }
            <div class="caption">
                <p style="font-size:8px;">@Model.AllCloudinaryFiles[i].Name</p>
            </div>
        </div>
    </div>
}